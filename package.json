    "@faceless-ui/window-info": "^2.1.2",
    "@payloadcms/db-mongodb": "^3.0.0-beta.73",
    "@payloadcms/next": "^3.0.0-beta.73",
    "@payloadcms/plugin-cloud": "^3.0.0-beta.73",
    "@payloadcms/richtext-lexical": "^3.0.0-beta.73",
    "@payloadcms/storage-s3": "^3.0.0-beta.73",
